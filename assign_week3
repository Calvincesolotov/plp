my_list = [] #create empty list
my_list.append(10) #append
my_list.append(20)
my_list.append(30)
my_list.append(40)
my_list.insert(1,15) # insert at second position
my_list.extend([50,60,70]) # extend the llsit 
my_list.pop() # remove last
my_list.sort()
def index_of_30():
    for value in my_list:
        if value == 30:
            print(my_list.index(30))
            
index_of_30()
         
print(my_list)